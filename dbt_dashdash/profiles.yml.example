dashdash_lab:
  target: dev
  outputs:
    dev:
      type: postgres
      host: "{{ env_var('PGHOST') }}"
      port: "{{ env_var('PGPORT', '5432') }}"
      user: "{{ env_var('PGUSER') }}"
      password: "{{ env_var('PGPASSWORD') }}"
      dbname: "{{ env_var('PGDATABASE') }}"
      schema: "{{ env_var('DBT_SCHEMA', env_var('PGUSER')) }}"
      threads: 4
      keepalives_idle: 0
      connect_timeout: 10
